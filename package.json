{
  "name": "kaspersky",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start-db": "docker run -p 6379:6379 redis",
    "start-server": "nodemon server.js",
    "start-client": "vue-cli-service serve",
    "start": "run-p -r start-db start-server start-client"
  },
  "dependencies": {
    "vue": "^2.5.21",
    "vue-router": "3.0.2",
    "vue-socket.io": "3.0.4",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "axios": "0.18.0",
    "babel-eslint": "^10.0.1",
    "body-parser": "1.18.3",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "express": "4.16.4",
    "nodemon": "1.18.9",
    "npm-run-all": "4.1.5",
    "redis": "2.8.0",
    "socket.io": "2.2.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
